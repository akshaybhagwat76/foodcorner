<!DOCTYPE html>
<html>
<head>
    <title>Registration Form</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Registration / Login form template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free WebDesigns for Nokia, Samsung, LG, Sony Ericsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Custom CSS -->
    <link href="@Url.Content("~/Content/user assets/")css/style.css" rel='stylesheet' type='text/css' />
    <!-- font CSS -->
    <!--
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link href="//fonts.googleapis.com/css?family=Archivo+Black" rel="stylesheet">-->
    <link href="//fonts.googleapis.com/css?family=Signika:300,400,700" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Roboto+Condensed:400,700" rel="stylesheet">

    <!--font CSS-->
    <script src="@Url.Content("~/Content/user assets/")js/jquery2.0.3.min.js"></script>

    <script>
        $(function () {
            $('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

            if (!screenfull.enabled) {
                return false;
            }



            $('#toggle').click(function () {
                screenfull.toggle($('#container')[0]);
            });
        });
    </script>
    <meta name="description" content="The description of my page" />
</head>
@Html.Raw(TempData["tmp"])

<body class="dashboard-page">
    <div class="main-grid">
        <div class="agile-grids">
            <!-- validation -->
            <div class="grids">
                <div class="progressbar-heading grids-heading">
                    <h2>registration form</h2>
                </div>

                <div class="forms-grids">
                    <div class="forms3">
                        <div class="w3agile-validation w3ls-validation">
                                <div class="validation-grids widget-shadow" data-example-id="basic-forms">
                                    <div class="input-info">
                                        <center><h3>Register Form :</h3></center>
                                    </div>
                                    <div class="form-body form-body-info">

                                        @*<form data-toggle="validator" novalidate="" action="#" method="post">*@
                                        @using (Html.BeginForm("restregi", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {

                                            <div class="form-group valid-form">
                                                <input type="text" class="form-control" id="inputName" placeholder="Username" name="runame" required="please" />

                                            </div>
                                            <div class="form-group has-feedback">
                                                <input type="text" class="form-control inputEmail" data-error="That email address is invalid" required="" placeholder="Email" name="remail" />

                                                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                <label for="p1" style="color:red;">@Html.Raw(TempData["u1"])</label>

                                            </div>
                                            <br />
                                            <div class="form-group has-feedback">
                                                <input type="password" class="form-control inputEmail" data-error="That email address is invalid" required="" placeholder="Password" name="rpwd" />

                                                <span class="glyphicon form-control-feedback" aria-hidden="true"></span>

                                            </div>
                                            <br />
                                            <div class="form-group">
                                                <input type="text" class="form-control" required="" placeholder="Description" name="rdesc" />

                                                <br />
                                                <input type="text" placeholder="Phone No" class="form-control" required="" name="rphno" />
                                                <br />
                                                <input type="text" placeholder="Address" class="form-control" required="" name="radd" />
                                                <br />
                                                <input type="file" id="file" name="file" class="form-control" />

                                            </div>


                                            <div class="form-group">
                                                <div class="checkbox">
                                                    <label>
                                                        <input type="checkbox" data-error="Before you wreck yourself" required="">
                                                        I have read and accept terms of use.
                                                    </label>
                                                    <div class="help-block with-errors"></div>
                                                </div>
                                            </div>
                                          <center style="margin-left:1000px;margin-top:10px;">  @Html.ActionLink("Login Here", "restlogin", "Home")</center>

                                            <button type="submit" class="btn-primary disabled">Register</button>


                                        }
                                        @*</form>*@
                                    </div>
                                </div>
                            </div>


                        </div>
                        <div class="clear"> </div>
                    </div>
                </div>



            </div>
            <!-- //validation -->
        </div>
    </div>
    <!-- footer -->
    <div class="footer">
        <p>© 2016 Registration / Login form . All Rights Reserved . Design by <a href="http://w3layouts.com/" target="_blank">W3layouts</a></p>
    </div>
    <!-- //footer -->
    <!-- input-forms -->


