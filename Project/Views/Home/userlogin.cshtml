<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Login panel</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/Content/admin page design/")bootstrap/css/bootstrap.min.css ">

    <!-- Font Awesome -->
    <link rel=" stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Content/admin page design/")dist/css/AdminLTE.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="@Url.Content("~/Content/admin page design/")plugins/iCheck/square/blue.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script type="text/javascript">

    </script>
    @Html.Raw(TempData["msg"])


</head>
<body class="hold-transition login-page">
    @Html.Raw(TempData["msg"])
    <div class="login-box">
        <div class="login-logo">
            <a href="../../index2.html"><b>User Login</b></a>
        </div>
        <!-- /.login-logo -->
        <div class="login-box-body">
            <p class="login-box-msg">Sign in to start your session</p>
            @*<form action="../../index2.html" method="post">*@
            @*<div class="form-group has-feedback">
                    <input type="email" class="form-control" placeholder="Email">
                    <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="password" class="form-control" placeholder="Password">
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>*@
            @using (Html.BeginForm("userlogin", "Home", FormMethod.Post))
            {
<script src="~/Scripts/jquery-3.1.1.min.js"></script>
                                
                <script type="text/javascript">
                    $(document).ready(function () {

                      //  alert("**Please login with valid username and password**");
                    });
                </script>
                <div class="form-group has-feedback">
                    <input type="text" placeholder="Email" name="t1" id="userid" class="form-control" />
                    @*  <span class="glyphicon glyphicon-envelope form-control-feedback"></span>*@

                </div>
                <div class="form-group has-feedback">
                    <input type="password" placeholder="Password" name="t2" class="form-control" />
                    @* <span class="glyphicon glyphicon-lock form-control-feedback"></span>*@

                </div>
                <div class="row">
                    <div class="col-xs-8">
                        <div class="checkbox icheck">
                            <label>
                                <input type="checkbox"> Remember Me
                            </label>
                        </div>
                    </div>
                    <!-- /.col -->
                    <div class="col-xs-4">
                        <button type="submit" class="btn btn-primary btn-block btn-flat" id="savedata"><span>Sign In</span></button>
                        @*@Html.ActionLink("Sign In", "Index",
                            new { controller = "AdminDashboard" },
                                new { @class = "btn btn-primary btn-block btn-flat" })*@
                    </div>
                    <!-- /.col -->
                </div>
                @Html.ActionLink("Forget Password","forgetpwd","Home")
            }

            <div class="social-auth-links text-center">
                <p>- OR -</p>
                <a href="#" class="btn btn-block btn-social btn-facebook btn-flat">
                    <i class="fa fa-facebook"></i> Sign in using
                    Facebook
                </a>
                <a href="#" class="btn btn-block btn-social btn-google btn-flat">
                    <i class="fa fa-google-plus"></i> Sign in using
                    Google+
                </a>
            </div>
            <!-- /.social-auth-links -->
            <a href="#">I forgot my password</a><br>
            <div class="text-center">
                @Html.ActionLink("Register a new membership", "userregi", "Home")
                <!-- /.login-box-body -->
            </div>
            <!-- /.login-box -->
            <!-- jQuery 2.2.3 -->
            <script src="@Url.Content("~/Content/admin page design/")plugins/jQuery/jquery-2.2.3.min.js"></script>
            <!-- Bootstrap 3.3.6 -->
            <script src="@Url.Content("~/Content/admin page design/")bootstrap/js/bootstrap.min.js"></script>
            <!-- iCheck -->
            <script src="@Url.Content("~/Content/admin page design/")plugins/iCheck/icheck.min.js"></script>
            <script>
                $(function () {
                    $('input').iCheck({
                        checkboxClass: 'icheckbox_square-blue',
                        radioClass: 'iradio_square-blue',
                        increaseArea: '20%' // optional
                    });
                });
            </script>

        </div>
    </div>
</body>
</html>
